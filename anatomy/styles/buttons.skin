.primary-button {
  background-color: #0070f3;
  color: white;
  padding: 10px 20px;
  border: none;
  border-radius: 4px;
  font-size: 1rem;
  cursor: pointer;
}

.secondary-button {
  background-color: white;
  color: #0070f3;
  padding: 10px 20px;
  border: 1px solid #0070f3;
  border-radius: 4px;
  font-size: 1rem;
  cursor: pointer;
}

